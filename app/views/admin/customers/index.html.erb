<h1>会員一覧</h1>
<table class="customers-table">
  <thead>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>会員ステータス</th>
    </tr>
  </thead>
  <tbody>
    <%= @customers.each do |customer| %>
    <tr>
      <td><%= customer.id %></td>
      <td><%= customer.last_name + customer.first_name %></td>
      <td><%= customer.email %></td>
      <td>ステータス</td>
    </tr>
    <% end %>
</tbody>
</table>

